
#!/bin/sh

module=$1
function=$2

if [ -z "$module" ] && [ -z "$function" ]
then
    echo "Requires argument(s). Eventually implement this to run all tests if none are chosen."
    echo "Running default."
    python -c "from Test_Integration_Functions import *; test_collect_counts_and_plot_two_matrixes()"
else
    python -c "from $module import *; $function()"
fi

